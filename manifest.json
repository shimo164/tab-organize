{
  "manifest_version": 3,
  "name": "Tab Organize",
  "version": "1.1.0",
  "description": "Merge and sort tabs from all windows. Switch back to the previously active tab.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "commands": {
    "switch-to-previous-tab": {
      "suggested_key": {
        "default": "Ctrl+E"
      },
      "description": "Switch to previously active tab"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
